						<!--		<viz.feeds:FeedItem id='categoryAxisFeed' uid="color" type="Dimension" name="operator" values="operator"/>-->
						<!--	</viz:feeds>-->
						<!--</viz:VizFrame>-->
					</l:flexContent>
				</l:FixFlex>
			</Page>
		</pages>
	</App>
</mvc:View>