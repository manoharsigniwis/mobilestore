<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="Mobile.Mobilestore.controller.Recharge"
	xmlns:html="http://www.w3.org/1999/xhtml" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form" xmlns:tnt="sap.tnt"
	xmlns:viz="sap.viz.ui5.controls" xmlns:viz.feeds="sap.viz.ui5.controls.common.feeds" xmlns:viz.data="sap.viz.ui5.data">
	<App>
		<pages>
			<Page  showHeader="false" >
				<tnt:ToolHeader class="toolhaeader">
					<core:Icon src="sap-icon://nav-back"  size="2rem" press="onNavBack"></core:Icon>
					<ToolbarSpacer></ToolbarSpacer>
					<Image src="https://images-na.ssl-images-amazon.com/images/I/71pw3Vc4KmL._SX355_.jpg" width="50px" height="50px"></Image>
					<Title text="Signiwis Mobiles" titleStyle="H1" level="H1" class="sapUiTinyMargin"/>
					<ToolbarSpacer></ToolbarSpacer>
					<Button text="Logout" type="Transparent"></Button>
					</tnt:ToolHeader>
				<l:FixFlex class="fixFlexHorizontal" vertical="false">
					<l:fixContent>
						<f:Form id="FormDisplay" editable="false">
							<f:layout>
								<f:ResponsiveGridLayout/>
							</f:layout>
							<f:formContainers>
								<f:FormContainer>
									<f:formElements>
										<f:FormElement>
											<f:fields>
												<tnt:InfoLabel id="il1" text="Mobile Number" renderMode="Narrow" colorScheme="5" displayOnly="true"/>
												<Input type="Number" editable="true" placeholder="Mobile Number"></Input>
											</f:fields>
										</f:FormElement>
										<f:FormElement>
											<f:fields>
												<tnt:InfoLabel text="Mobile Operator" renderMode="Narrow" colorScheme="5" displayOnly="true"/>
												<ComboBox id="i" items="{data>/recharge}" placeholder="Mobile Operator">
													<core:Item key="{data>operator}" text="{data>operator}"/>
												</ComboBox>
											</f:fields>
										</f:FormElement>
										<f:FormElement>
											<f:fields>
												<tnt:InfoLabel text="Amount" renderMode="Narrow" colorScheme="5" displayOnly="true"/>
												<Input id="i1" type="Number" editable="true" placeholder="Amount"></Input>
											</f:fields>
										</f:FormElement>
										<f:FormElement>
											<f:fields>
												<Button text="Recharge" type="Emphasized" press="onRecharge"></Button>
											</f:fields>
										</f:FormElement>
									</f:formElements>
								</f:FormContainer>
							</f:formContainers>
						</f:Form>
					</l:fixContent>
					<l:flexContent>
						<FlexBox alignItems="Start">
							<items>
								<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="JIO" press="press">
									<TileContent footer="Remaining Currency">
										<NumericContent value="{data>/recharge/1/balance}" icon="Images/download.png"/>
									</TileContent>
								</GenericTile>
								<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="AIRTEL" press="press">
									<TileContent footer="Remaining Currency">
										<NumericContent value="{data>/recharge/1/balance}" icon="https://logos-download.com/wp-content/uploads/2016/07/Airtel_logo.png"/>
									</TileContent>
								</GenericTile>
								<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="VODAFONE" press="press">
									<TileContent footer="Remaining Currency">
										<NumericContent value="{data>/recharge/2/balance}"
											icon="https://diylogodesigns.com/wp-content/uploads/2016/05/Vodafone-Logo-png-download-768x552.png"/>
									</TileContent>
								</GenericTile>
								<!--<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="BSNL" press="press">-->
								<!--	<TileContent footer="Remaining Currency">-->
								<!--		<NumericContent value="{data>/recharge/3/balance}"-->
								<!--			icon="http://www.logosvectorfree.com/wp-content/uploads/2017/12/bsnl-Logo-Icon-Vector-Free-Download-964x1024.png"/>-->
								<!--	</TileContent>-->
								<!--</GenericTile>-->
							</items>
						</FlexBox>
						<!--<NumericContent value="{data>/recharge/1/balance}" scale="MM"-->
						<!--	icon="data:image/png;base64,"-->
						<!--	valueColor="Good"  class="sapUiSmallMargin" press="press"/>-->
