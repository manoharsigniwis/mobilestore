<mvc:View controllerName="Mobile.Mobilestore.controller.Home" xmlns:l="sap.ui.layout" xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m" xmlns:tnt="sap.tnt" height="100%">
	<Page showHeader="false">
		<tnt:ToolHeader class="toolhaeader">
			<Image src="./Images/margo.png" width="200px" height="95px"></Image>
			<ToolbarSpacer></ToolbarSpacer>
			<Title text="Signiwis Mobiles" titleStyle="H1" level="H1" class="sapUiTinyMargin"/>
			<ToolbarSpacer></ToolbarSpacer>
			<Button text="Logout" type="Transparent">
				<layoutData>
					<OverflowToolbarLayoutData priority="NeverOverflow"/>
				</layoutData>
			</Button>
			<Button text="next" press="onPress" type="Transparent"></Button>
		</tnt:ToolHeader>
		<SplitApp id="SplitAppDemo" initialDetail="detail" initialMaster="master" orientationChange="onOrientationChange">
			<detailPages>
				<Page id="detail" showHeader="false">
					<content>
						<!--<Label text="Detail page 1"/>-->
						<!--<Button text="Go to Detail page2" press="onPressNavToDetail"/>-->
						<l:BlockLayout background="Dashboard">
							<l:BlockLayoutRow>
								<l:BlockLayoutCell>
									<Title text="Products Sold" titleStyle="H1" level="H1" class="sapUiTinyMargin"/>
									<NumericContent value="6666" valueColor="Critical" class="sapUiTinyMargin"/>
								</l:BlockLayoutCell>
								<l:BlockLayoutCell>
									<Title text="Recharge Done" titleStyle="H1" level="H1" class="sapUiTinyMargin"/>
									<NumericContent value="6666" valueColor="Critical" class="sapUiTinyMargin"/>
								</l:BlockLayoutCell>
								<l:BlockLayoutCell>
									<Title text="Transactions" titleStyle="H1" level="H1" class="sapUiTinyMargin"/>
									<NumericContent value="6666" valueColor="Critical" class="sapUiTinyMargin"/>
								</l:BlockLayoutCell>
							</l:BlockLayoutRow>
						</l:BlockLayout>
						<l:Grid defaultSpan="XL4 L4 M6 S12">
							<List id="notificationList">
								<NotificationListItem title="Notification 1" description="My Notification"></NotificationListItem>
								<NotificationListItem title="Notification 2" description="My Notification"></NotificationListItem>
								<NotificationListItem title="Notification 3" description="My Notification"></NotificationListItem>
								<NotificationListItem title="Notification 4" description="My Notification"></NotificationListItem>
							</List>
						</l:Grid>
					</content>
				</Page>
			</detailPages>
			<masterPages>
				<Page id="master" showHeader="false">
					<content>
						<l:BlockLayout>
							<l:BlockLayoutRow>
								<l:BlockLayoutCell backgroundColorSet="ColorSet7" backgroundColorShade="ShadeB">
									<l:titleLink>
										<Link class="textcolor" subtle="true" text="Billing"
											href="https://webidetesting0687364-p2000691080trial.dispatcher.hanatrial.ondemand.com/webapp/index.html?hc_orionpath=%2FDI_webide_di_workspace3uuuazn57vw5vy52%2Fmobilestore&neo-di-affinity=BIGipServer~jpaas_folder~disapwebide.hanatrial.ondemand.com+%21TxLmYhI%2BEH6zqLyn5pxGS30xvFkQ81eNJrkVo0H7aSUBUbiWhmGkLB7wXgJ2dEURj%2FOavPPNfKwV&origional-url=index.html&sap-ui-appCacheBuster=..%2F&sap-ui-xx-componentPreload=off#/Billing"/>
									</l:titleLink>
									<core:Icon src="sap-icon://sales-order" height="70px"/>
								</l:BlockLayoutCell>
							</l:BlockLayoutRow>
							<l:BlockLayoutRow>
								<l:BlockLayoutCell backgroundColorSet="ColorSet7" backgroundColorShade="ShadeC">
									<l:titleLink>
										<Link subtle="true" class="textcolor" text="Recharge"
											href="https://webidetesting0687364-p2000691080trial.dispatcher.hanatrial.ondemand.com/webapp/index.html?hc_orionpath=%2FDI_webide_di_workspace3uuuazn57vw5vy52%2Fmobilestore&neo-di-affinity=BIGipServer~jpaas_folder~disapwebide.hanatrial.ondemand.com+%21TxLmYhI%2BEH6zqLyn5pxGS30xvFkQ81eNJrkVo0H7aSUBUbiWhmGkLB7wXgJ2dEURj%2FOavPPNfKwV&origional-url=index.html&sap-ui-appCacheBuster=..%2F&sap-ui-xx-componentPreload=off#/Recharge"/>
									</l:titleLink>
									<core:Icon src="sap-icon://lead" height="70px"/>
								</l:BlockLayoutCell>
							</l:BlockLayoutRow>
							<l:BlockLayoutRow>
								<l:BlockLayoutCell backgroundColorSet="ColorSet7" backgroundColorShade="ShadeB">
									<l:titleLink>
										<Link subtle="true" class="textcolor" text="Service"
											href="https://webidetesting0687364-p2000691080trial.dispatcher.hanatrial.ondemand.com/webapp/index.html?hc_orionpath=%2FDI_webide_di_workspace3uuuazn57vw5vy52%2Fmobilestore&neo-di-affinity=BIGipServer~jpaas_folder~disapwebide.hanatrial.ondemand.com+%21TxLmYhI%2BEH6zqLyn5pxGS30xvFkQ81eNJrkVo0H7aSUBUbiWhmGkLB7wXgJ2dEURj%2FOavPPNfKwV&origional-url=index.html&sap-ui-appCacheBuster=..%2F&sap-ui-xx-componentPreload=off#/serviceBilling"/>
									</l:titleLink>
									<core:Icon src="sap-icon://crm-service-manager" height="70px"/>
								</l:BlockLayoutCell>
							</l:BlockLayoutRow>
							<l:BlockLayoutRow>
								<l:BlockLayoutCell backgroundColorSet="ColorSet7" backgroundColorShade="ShadeC">
									<l:titleLink>
										<Link subtle="true" class="textcolor" text="Inventory"
											href="https://webidetesting0687364-p2000691080trial.dispatcher.hanatrial.ondemand.com/webapp/index.html?hc_orionpath=%2FDI_webide_di_workspace3uuuazn57vw5vy52%2Fmobilestore&neo-di-affinity=BIGipServer~jpaas_folder~disapwebide.hanatrial.ondemand.com+%21TxLmYhI%2BEH6zqLyn5pxGS30xvFkQ81eNJrkVo0H7aSUBUbiWhmGkLB7wXgJ2dEURj%2FOavPPNfKwV&origional-url=index.html&sap-ui-appCacheBuster=..%2F&sap-ui-xx-componentPreload=off#/Inventory"/>
									</l:titleLink>
									<core:Icon src="sap-icon://product" height="70px"/>
								</l:BlockLayoutCell>
							</l:BlockLayoutRow>
							<l:BlockLayoutRow>
								<l:BlockLayoutCell backgroundColorSet="ColorSet7" backgroundColorShade="ShadeB">
									<!--<Title text="Products Sold" titleStyle="H1" level="H1" class="sapUiTinyMargin"/>-->
									<l:titleLink>
										<Link subtle="true" class="textcolor" text="Transactions"
											href="https://webidetesting0687364-p2000691080trial.dispatcher.hanatrial.ondemand.com/webapp/index.html?hc_orionpath=%2FDI_webide_di_workspace3uuuazn57vw5vy52%2Fmobilestore&neo-di-affinity=BIGipServer~jpaas_folder~disapwebide.hanatrial.ondemand.com+%21TxLmYhI%2BEH6zqLyn5pxGS30xvFkQ81eNJrkVo0H7aSUBUbiWhmGkLB7wXgJ2dEURj%2FOavPPNfKwV&origional-url=index.html&sap-ui-appCacheBuster=..%2F&sap-ui-xx-componentPreload=off#/Transaction"/>
									</l:titleLink>
									<core:Icon src="sap-icon://collections-insight" height="70px"/>
								</l:BlockLayoutCell>
							</l:BlockLayoutRow>
						</l:BlockLayout>
					</content>
				</Page>
			</masterPages>
		</SplitApp>
		<footer>
			<OverflowToolbar id="otbFooter">
				<!--<Title text="Signiwis Mobiles" deccription="hi" titleStyle="H1" level="H1" class="sapUiTinyMargin"/>-->
				<ObjectIdentifier title="Signiwis Mobiles" text="The Mobile World"/>
				<ToolbarSpacer/>
				<ObjectIdentifier title="Designed by:" text="SM Web Tecchnologies"/>
			</OverflowToolbar>
		</footer>
	</Page>
</mvc:View>