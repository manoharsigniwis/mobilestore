<mvc:View xmlns:core="sap.ui.core" xmlns:tnt="sap.tnt" xmlns:mvc="sap.ui.core.mvc" xmlns:f="sap.ui.layout.form" xmlns="sap.m"
	xmlns:l="sap.ui.layout" controllerName="Mobile.Mobilestore.controller.Billing" xmlns:html="http://www.w3.org/1999/xhtml">
	<Page showHeader="false">
		<tnt:ToolHeader class="toolhaeader">
			<Image src="./Images/margo.png" width="200px" height="95px"></Image>
			<ToolbarSpacer></ToolbarSpacer>
			<Title text="Signiwis Mobiles" titleStyle="H1" level="H1" class="sapUiTinyMargin"/>
			<ToolbarSpacer></ToolbarSpacer>
			<Button text="Logout" type="Transparent">
				<layoutData>
					<OverflowToolbarLayoutData priority="NeverOverflow"/>
				</layoutData>
			</Button>
			<Button text="back" press="onNavBack" type="Transparent"></Button>
			<Button text="Nextb" press="onPress"></Button>
		</tnt:ToolHeader>
		<VBox class="sapUiSmallMargin">
			<f:SimpleForm id="SimpleFormToolbar" title="Sales Invoice" editable="true" layout="ResponsiveGridLayout" labelSpanXL="4" labelSpanL="4"
				labelSpanM="4" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="0" emptySpanL="3" emptySpanM="0" emptySpanS="0" columnsXL="2"
				columnsL="1" columnsM="1" singleContainerFullSize="false" ariaLabelledBy="Title1">
				<f:content>
					<Label text=" Order ID"/>
					<Input id="oid" value="" editable="false"/>
					<Label text="Date:"/>
					<DatePicker id="date" displayFormat="long" editable="false"/>
					<Label text=" Customer Name"/>
					<Input id="name" value="" placeholder="Name"/>
					<Label text="Mobile No."/>
					<Input id="mobile" type="Number" value="" placeholder="Number"></Input>
					<Label text="Category"/>
					<Input value="" placeholder="Category"/>
					<Label text="Product No"/>
					<Input id="pno" value="" placeholder="Product No"/>
					<Label text="Brand"/>
					<ComboBox id="brand" items="{data>/brands}" change="onBrandChange">
						<items>
							<core:Item text="{data>brand}" key="{data>brand}"/>
						</items>
					</ComboBox>
					<Label text="Model"/>
					<ComboBox id="model" items="{data>/mobiles}" change="onModelChange">
						<items>
							<core:Item text="{data>model}" key="{data>model}"/>
						</items>
					</ComboBox>
					<Label text="Price"/>
					<Input id="price" value="{data>/newValue}"/>
					<Label text="Coupon Code"/>
					<ComboBox id="coupon">
						<!--<items>-->
						<core:Item text="Save@200"/>
						<core:Item text="FestiveOffer"/>
						<core:Item text="BigSale"/>
						<core:Item text="Offer@30"/>
						<core:Item text="Flat@50"/>
						<!--</items>-->
						<layoutData>
							<l:GridData span="XL3 L3 M5 S12"/>
						</layoutData>
					</ComboBox>
					<Button text="Apply Coupon" press="onCoupon"></Button>
					<Label text="Total Amount"/>
					<Input id="tprice" value="37999"/>
					<Label text=""/>
					<Button text="Submit" press="onSubmit"></Button>
				</f:content>
			</f:SimpleForm>
		</VBox>
	</Page>
</mvc:View>